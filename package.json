{
  "name": "practice-gatsby-monorepo",
  "private": true,
  "description": "Practice with gatsby and monorepo architecture.",
  "version": "0.1.0",
  "author": "Michael Leonard <mtleonard120@gmail.com>",
  "dependencies": {
    "axios": "^0.19.2",
    "gatsby": "^2.23.12",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "lerna": "3.22.1",
    "prettier": "2.0.5"
  },
  "scripts": {
    "build:site1": "npm --prefix packages/site1 run build",
    "build:site2": "npm --prefix packages/site2 run build",
    "site1": "cd packages/site1 && npm run develop",
    "site2": "cd packages/site2 && npm run develop",
    "all": "npm-run-all --parallel run:site1 run:site2"
  },
  "workspaces": [
    "packages/*"
  ]
}
